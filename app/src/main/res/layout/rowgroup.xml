<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:id="@+id/LinearLayout01"    android:layout_width="fill_parent"    android:layout_height="wrap_content"    android:orientation="vertical"    >                    <TextView                        android:id="@+id/Text_ID"                        android:layout_width="0dip"                        android:layout_height="0dip"                        android:focusable="false"                        android:text="ID"                        android:textSize="0dip" />                                        <TextView                        android:id="@+id/TextNAME"                        android:layout_width="fill_parent"                        android:layout_height="wrap_content"                        android:focusable="false"                        android:paddingTop="5dip"                        android:paddingRight="5dip"                        android:paddingLeft="5dip"                        android:text="DISPLAY_NAME"                        android:textSize="25dip" android:ellipsize="end"/>                    <TextView                        android:id="@+id/TextGroup"                        android:layout_width="fill_parent"                        android:layout_height="wrap_content"                        android:focusable="false"                        android:paddingLeft="10dip"                        android:paddingBottom="5dip"                        android:text="GROUP"                        android:textSize="10dip" android:ellipsize="end"/>	</LinearLayout>