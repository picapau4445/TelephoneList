<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:id="@+id/LinearLayout01"    android:layout_width="fill_parent"    android:layout_height="wrap_content"    android:orientation="horizontal" >                    <CheckBox                        android:id="@+id/checkBox1"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text=""                        android:layout_alignParentRight="true"                        android:paddingRight="5dip"                        />
                    <LinearLayout                        android:id="@+id/linearLayout1"                        android:layout_width="match_parent"                        android:layout_height="match_parent"                        android:orientation="vertical" >
                        <TextView                            android:id="@+id/Text_ID"                            android:layout_width="0dip"                            android:layout_height="0dip"                            android:focusable="false"                            android:text="ID"                            android:textSize="0dip" />
                        <TextView                            android:id="@+id/TextNAME"                            android:layout_width="match_parent"                            android:layout_height="match_parent"                            android:focusable="false"                            android:gravity="center_vertical"                            android:paddingLeft="5dip"                            android:paddingRight="5dip"                            android:paddingTop="5dip"                            android:text="DISPLAY_NAME"                            android:textSize="25dip" />	                    <TextView	                        android:id="@+id/TextType"	                        android:layout_width="fill_parent"	                        android:layout_height="wrap_content"	                        android:focusable="false"	                        android:paddingLeft="10dip"	                        android:paddingBottom="5dip"	                        android:text="TYPE"	                        android:textSize="10dip" android:ellipsize="end"/>                                            </LinearLayout>	</RelativeLayout>